<launch> 

   <arg name="no_robot" default="false"/>

  <!-- Fake TFs -->
  <group if="$(arg no_robot)">
    <node pkg="tf" type="static_transform_publisher" name="base_link_to_velo" args="0 0 1 0 0 0 base_link velodyne 10" />
    <node pkg="tf" type="static_transform_publisher" name="map_to_base_link" args="0 0 0 0 0 0 map base_link 100" />
  </group>

<!-- OS1 Driver -->
  <include file="$(find ouster_ros)/os1.launch">
    <arg name="os1_hostname" value="192.168.1.50" />
    <arg name="os1_udp_dest" value="192.168.1.123" />
    <arg name="lidar_mode" value="512x10" />
  </include>

<!-- Intel Real Sense Driver -->
  <!-- <include file="$(find rasberry_hri)/launch/d415_camera.launch"/> -->
  <include file="$(find realsense2_camera)/launch/rs_camera.launch"/>

  <!-- Set to match the cameras that are launched in the include file (camera launch script) -->
  <arg name="topic_prefixes" default="rsense"/> <!-- Separate topic_prefixes with commas -->


<!-- Thermal Camera -->
<!-- <include file="$(find optris_drivers)/launch/optris_drivers.launch"/> -->


</launch>
