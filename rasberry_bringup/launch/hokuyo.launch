<?xml version="1.0" ?>

<launch>
  <arg name="frame_id" default="hokuyo" />
  <arg name="ip_address" default="192.168.0.10" />
  <arg name="angle_min" default="-1.570796" />
  <arg name="angle_max" default="1.570796" />
  <arg name="node_name" default="urg_node" />

  <node pkg="urg_node" type="urg_node" name="$(arg node_name)" >
    <param name="frame_id" value="$(arg frame_id)" />
    <param name="ip_address" value="$(arg ip_address)" />
    <param name="angle_min" value="$(arg angle_min)" />
    <param name="angle_max" value="$(arg angle_max)" />
  </node>

</launch>
